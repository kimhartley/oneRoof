  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Boyd-Thurston/boilerplate-react-redux.git"
  },
  "keywords": [
    "boilderplate",
    "react",
    "redux",
    "authorisation"
  ],
  "author": "Boyd Thurston",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Boyd-Thurston/boilerplate-react-redux/issues"
  },
  "homepage": "https://github.com/Boyd-Thurston/boilerplate-react-redux#readme",
  "scripts": {
